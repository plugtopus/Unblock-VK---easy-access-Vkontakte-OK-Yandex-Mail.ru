{
   "background": {
      "scripts": [ "js/jquery.min.js", "js/default-domains.js", "js/app.js", "js/proxy.js"]
   },
   "browser_action": {
      "default_title": "__MSG_title__"
   },
   "content_scripts": [ {
      "js": [ "/js/jquery.min.js"],
      "matches": [ "*://vk.com/*" ]
   } ],
   "default_locale": "en",
   "description": "__MSG_desc__",
   "icons": {
      "16": "img/16.png",
      "32": "img/32.png",
      "48": "img/48.png",
      "128": "img/128.png"
   },
   "manifest_version": 2,
   "name": "__MSG_name__",
   "short_name": "__MSG_shortname__",
   "permissions": [ "webRequest", "webRequestBlocking", "*://*/*", "proxy", "storage", "tabs" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "2018.07.04.1"
}
